structure list
{
    int value;
    list next;

    list(int value)
    {
        this.value = value;
        this.next = null;
    }

    void append(list other)
    {
        if (this.next == null) {
            this.next = other;
        } else {
            this.next.append(other);
        }
    }

    void print()
    {
        list cur = this;
        while (cur != null) {
            print(cur.value);
            cur = cur.next;
        }
    }
}

void main()
{
    list x = [1, 2, 3, 4];

    x.print();

    
}