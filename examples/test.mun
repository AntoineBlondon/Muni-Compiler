import env.print(int) -> void;


structure list
{
    static int MAX_LENGTH = 10;
    int value;
    list next;


    list(int value)
    {
        this.value = value;
        this.next = null;
    }

    

    void append(list other)
    {
        if (this.next == null) {
            this.next = other;
        } else {
            this.next.append(other);
        }
    }

    void print()
    {
        list cur = this;
        while (cur != null) {
            print(cur.value);
            cur = cur.next;
        }
    }
}

structure box
{
    int value;

    box(int value)
    {
        this.value = value;
    }
}

void print_value<T>(T a)
{
    print(a.value);
}

void main()
{
    box a_box = box(2);
    list a_list = list(1);
    print_value<box>(a_box);
    print_value<list>(a_list);
}