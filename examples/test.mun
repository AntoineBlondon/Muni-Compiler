structure list
{
    static int MAX_LENGTH = 10;
    int value;
    list next;


    list(int value)
    {
        this.value = value;
        this.next = null;
    }

    

    void append(list other)
    {
        if (this.next == null) {
            this.next = other;
        } else {
            this.next.append(other);
        }
    }

    void print()
    {
        list cur = this;
        while (cur != null) {
            print(cur.value);
            cur = cur.next;
        }
    }
}




void main()
{
    list x = [1, 2, 3, 4];
    x.append([5, 6, 7]);

    print(list.MAX_LENGTH);

    
}