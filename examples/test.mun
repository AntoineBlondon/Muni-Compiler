structure list
{
    int value;
    list next;

    list(int value)
    {
        this.value = value;
        this.next = null;
    }

    void append(list other)
    {
        if (this.next == null) {
            this.next = other;
        } else {
            this.next.append(other);
        }
    }
}

void main()
{
    list x = list(2);
    list y = list(1);
    x.append(y);
    
    list cur = x;
    while (cur != null) {
        print(cur.value);
        cur = cur.next;
    }
}